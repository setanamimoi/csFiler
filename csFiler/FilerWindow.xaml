<Window x:Class="csFiler.FilerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FilerWindow" Height="64" Width="432" Loaded="Window_Loaded" Deactivated="Window_Deactivated">
    <Grid>
        <TextBox Name="processTextBox" PreviewKeyDown="processTextBox_PreviewKeyDown" />
        <Popup Name="intellisence">
            <ListBox Name="intellisenceListBox" PreviewKeyDown="intellisenceListBox_PreviewKeyDown" />
        </Popup>
    </Grid>
</Window>
